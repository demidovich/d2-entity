{
    "name": "demidovich/php-blues",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "data mapper",
        "ddd",
        "domain driven design"
    ],
    "autoload": {
        "psr-4": {
            "Blues\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
